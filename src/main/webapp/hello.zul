<?xml version="1.0" encoding="UTF-8"?>
<window id="demo" use="com.oxseed.zk.sandbox.DemoWindow"> 
	<groupbox mold="3d" width="100%">
		<caption label="View"><button label="Reload" height="18px" use="com.oxseed.zk.sandbox.ReloadButton"/></caption>
		<window id="view">
		</window>
	</groupbox>
	<groupbox id="tryView" mold="3d" open="false" width="100%">
