		<textbox id="codeView" class="code" rows="10" width="100%" use="com.oxseed.zk.sandbox.CodeView">
			<attribute name="value"><![CDATA[
<window title="My First Window" border="normal" width="200px">
Hello, World!
</window>
			]]></attribute>
		</textbox>
	</groupbox>
</window>
